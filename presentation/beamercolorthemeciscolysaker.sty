\mode<presentation>

% Define some Cisco colors
\definecolor{LightBlue}{RGB}{205,235,249}
\definecolor{Blue}{RGB}{0,188,235}
\definecolor{DarkBlue}{RGB}{30,68,113}
\definecolor{DarkerBlue}{RGB}{13,39,77}
\definecolor{Green}{RGB}{110,190,74}
\definecolor{Grey}{RGB}{103,103,103}
