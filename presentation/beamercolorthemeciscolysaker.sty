% Footline of regular frames
\setbeamercolor*{footline}{fg=Grey}

\setbeamercolor*{alerted text}{fg=Red}

\setbeamercolor{itemize item}{fg=Grey}
\setbeamercolor{normal text}{fg=Grey}

